# Path
export PATH="/usr/local/bin:/usr/local/sbin:~/bin:$PATH"

# PythonPath
export PYTHONPATH="$PYTHONPATH:/Users/ally/Projects/bin"

# Colours
export CLICOLOR=1
export LSCOLORS=ExFxBxDxCxegedabagacad
alias ls='ls -GFh'

# Aliases
alias ll='ls -l'
alias la='ls -al'


# Prompt
export PS1="\u@\h: \w > "
export PS2="+ "


# Homebrew
export HOMEBREW_NO_ANALYTICS=1
